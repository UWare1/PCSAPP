<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/FragmentDoctorUser"
    android:elevation="5dp"
    tools:context=".UserDoctorFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="32dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="32dp"
            android:background="@drawable/background_menu"
            android:elevation="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <TextView
                android:id="@+id/FullnameProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Chusana Nongkew"
                android:fontFamily="@font/promptbold"
                android:textSize="21sp"
                android:textStyle="bold"
                android:textColor="#000000"
                android:layout_marginTop="64dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/EmailProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nongchompoo"
                android:fontFamily="@font/promptlight"
                android:textColor="#828181"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/FullnameProfile" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical"
                android:layout_marginStart="24dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="24dp"
                android:layout_marginBottom="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/EmailProfile"
                app:layout_constraintVertical_bias="0.0">
                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scrollbarSize="1dp"
                    android:scrollbars="vertical"
                    android:scrollbarThumbVertical="@drawable/thumbscroll">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="vertical">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/C1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:background="@drawable/item_background">

                            <ImageView
                                android:id="@+id/profile11"
                                android:layout_width="43dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="8dp"
                                android:background="@drawable/user"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/profile12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/profile"
                                android:fontFamily="@font/promptbold"
                                android:textColor="#000000"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                app:layout_constraintStart_toEndOf="@+id/profile11"
                                app:layout_constraintTop_toTopOf="@+id/profile11" />
                            <Button
                                android:id="@+id/EditProfile"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00F6F6F6"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/C2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:background="@drawable/item_background">

                            <ImageView
                                android:id="@+id/secure11"
                                android:layout_width="43dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="8dp"
                                android:background="@drawable/insurance"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                />

                            <TextView
                                android:id="@+id/secure12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/security"
                                android:fontFamily="@font/promptbold"
                                android:textColor="#000000"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                app:layout_constraintStart_toEndOf="@+id/secure11"
                                app:layout_constraintTop_toTopOf="@+id/secure11" />
                            <Button
                                android:id="@+id/SecurityProfile"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00F6F6F6"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/C3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:background="@drawable/item_background">

                            <ImageView
                                android:id="@+id/language11"
                                android:layout_width="43dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="8dp"
                                android:background="@drawable/translation"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                />

                            <TextView
                                android:id="@+id/language12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/language"
                                android:fontFamily="@font/promptbold"
                                android:textColor="#000000"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                app:layout_constraintStart_toEndOf="@+id/language11"
                                app:layout_constraintTop_toTopOf="@+id/language11" />
                            <Button
                                android:id="@+id/LanguageProfile"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00F6F6F6"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/C4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:background="@drawable/item_background">

                            <ImageView
                                android:id="@+id/set11"
                                android:layout_width="43dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="8dp"
                                android:background="@drawable/gear"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                />

                            <TextView
                                android:id="@+id/set12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/setting"
                                android:fontFamily="@font/promptbold"
                                android:textColor="#000000"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                app:layout_constraintStart_toEndOf="@+id/set11"
                                app:layout_constraintTop_toTopOf="@+id/set11" />
                            <Button
                                android:id="@+id/SettingProfile"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00F6F6F6"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/C5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:background="@drawable/item_background">

                            <ImageView
                                android:id="@+id/help11"
                                android:layout_width="43dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="8dp"
                                android:background="@drawable/information"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                />

                            <TextView
                                android:id="@+id/help12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/about"
                                android:fontFamily="@font/promptbold"
                                android:textColor="#000000"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                app:layout_constraintStart_toEndOf="@+id/help11"
                                app:layout_constraintTop_toTopOf="@+id/help11" />
                            <Button
                                android:id="@+id/AboutProfile"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00F6F6F6"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/C6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginBottom="12dp"
                            android:background="@drawable/item_background">

                            <ImageView
                                android:id="@+id/log11"
                                android:layout_width="43dp"
                                android:layout_height="42dp"
                                android:layout_marginStart="8dp"

                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/logout" />

                            <TextView
                                android:id="@+id/log12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="24dp"
                                android:layout_marginTop="10dp"
                                android:text="@string/logout"
                                android:fontFamily="@font/promptbold"
                                android:textColor="#000000"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                app:layout_constraintStart_toEndOf="@+id/log11"
                                app:layout_constraintTop_toTopOf="@+id/log11" />
                            <Button
                                android:id="@+id/Logout"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00F6F6F6"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/ProfileImage"
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:elevation="8dp"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout"
            android:background="@drawable/iconprofile" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>